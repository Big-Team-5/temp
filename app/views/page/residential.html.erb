			</header>

			<!-- MODELS BOXES 1 -->
			<div class="row">
				<div class="col-md-4 col-6">
					<div class="text-center">
						<%= image_tag("/assets/RE/Residential page/PVE30.jpg",class:"img-fluid", width:"300", height:"200", alt:"")%> 
						<h4>RE-30</h4>
						<p>Single Passenger Residential Elevator</p>
					</div>
				</div>
				<div class="col-md-4 col-6">
					<div class="text-center">
						<%= image_tag("/assets/RE/Residential page/PVE37.jpg",class:"img-fluid", width:"300", height:"200", alt:"")%>
						<h4>RE-37</h4>
						<p>Two-Passenger Home Elevator</p>
					</div>
				</div>
				<div class="col-md-4 col-6">
					<div class="text-center">
						<%= image_tag("/assets/RE/Residential page/PVE52sm.jpg",class:"img-fluid", width:"300", height:"200", alt:"")%>
						<h4>RE-52</h4>
						<p>Three-Passenger Wheelchair Accessible Home Elevators</p>
					</div>
				</div>
			</div>
			<!-- /MODELS BOXES 1 -->


			<!-- MODELS BOXES 2 -->
			<div class="row">
				<div class="col-md-4 col-6">
					<div class="text-center">
						<%= image_tag("/assets/RE/Residential page/elevator_Boat.jpg",class:"img-fluid", width:"300", height:"200", alt:"")%>
						<h4>Yacht/Marine Elevators</h4>
						<p>Space-Saving Elevators for Yacht + Marine Applications<br>
						<%= link_to "Contact us for more informations", "#{root_path}#contact" %>
					</div>
				</div>
				<div class="col-md-4 col-6">
					<div class="text-center">
						<%= image_tag("/assets/RE/Residential page/shutterstock_1920x980.jpg",class:"img-fluid", width:"300", height:"200", alt:"")%>
						<h4>Multi-Residential</h4>
