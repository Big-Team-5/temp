<div id="wrapper">

	<!-- HOME -->
	<section id="home" style="padding-top: 100px">
				
		<div class="container text-center">
			
			<h1 class="m-0 fs-40 fw-300 wow fadeInUp" data-wow-delay="0.4s">Rocket Elevators Can Make Your Dream Come True</h1>
			<p class="lead font-lato fs-20 wow fadeInUp" data-wow-delay="0.7s">Rocket Elevators is the World’s ONLY Home Elevators Driven By Air</p>
			
			<div class="mt-100 mb-10">
				<a href="#models" class="btn btn-3d btn-lg btn-primary"></i>MODELS</a>
				<span class="fs-17 hidden-xs-down">&nbsp; OR &nbsp;</span>
				<%= link_to "PRINCING", "/quote_forms/new", class: "btn btn-3d btn-danger" %>
				<hr>
				<a href="#testimonials" style="font-weight: 500;font-size: 15px;color: lime;font-style: italic;">View our clients testimonials</a>
			</div>


			<div class="row mt-150 ml-200 text-left">

				<div class="col-md-6 col-sm-8 mt-30 mb-5">
					<%= image_tag("/assets/RE/Residential page/Residential-Elevators-PVE30-1.jpg",class:"img-fluid", width:"500", height:"300", alt:"")%> 
				</div>

				<div class="col-md-4 col-sm-12 mt-30">
					<h3 class="fw-400">Why <span style="color:lime; font-style:italic">Work with US</span></h3>
					<p style="padding-top: 15px">We can provide you with the latest technology in residential elevators.<br> By using pneumatic technology to generate lift, our home elevators smoothly move between floors while requiring much less space and energy than traditional residential elevators.<br>
					<br>We can take everything in charge from beginning to your first lift</p>
					<p>Easy installation without any major home improvement can be achieved with our products.</p>
					<hr/>
					<hr/>
					<!-- <ul class="list-unstyled list-icons"> -->
						<p class="list-icons">
							<span style="color:lime; font-style:italic;">
							<i class="fa fa-check"></i> Panoramic Design<br>
							<i class="fa fa-check"></i> Space Saving<br>
							<i class="fa fa-check"></i> Fast Installation<br>
							<i class="fa fa-check"></i> Pneumatic Technology<br>
							<i class="fa fa-check"></i> Little Maintenance<br>
							</span>
						</p>
					<!-- </ul> -->
				
				</div>
			</div>

		</div>

	</section>
